<!DOCTYPE html>
<html lang="en">
  
  <head>
	<% include head %>
    <title>Page Response Analysis</title>
  </head>
  
  <body>	
	<% include nav %>
	<div class="container-lg">
		<div class="row">
			<div class="col-lg-12"   >
    			<h3>Page Response Analysis</h3>
				<hr class="featurette-divider">
    			<form id="form" class="form-inline" role="form">
					<div class="form-group">
						<label for="field" class="control-label">Show</label>
						<select class="form-control" name="field" id="field">
							<option value="tt">Total Time</option>
							<option value="rd">Redirector Time</option>
							<option value="dns">DNS Time</option>
							<option value="con">Connection Time</option>
							<option value="rq">Server Time</option>
							<option value="rs">Download Time</option>
							<option value="dom">DOM Time</option>
							<option value="ld">Load Time</option>
							<option value="cnt">Transactions</option>
						</select>
					</div>
				  
					<div class="form-group">
						<label for="interval" class="control-label">Interval</label>
						<select class="form-control" name="interval" id="interval">
							<option value="page.min">Minute</option>
							<option value="page.hour">Hour</option>
							<option value="page.day">Day</option>
						</select>
					</div>
					
					<div class="form-group">
						<label for="fromTs" class="control-label">FromTime</label>
						<input type="text" class="form-control" name="fromTs" id="fromTs" value="<%= moment().format('YYYY-MM-DD') %>">
					</div>
   
                  <div class="form-group">
    				  <label for="toTs" class="control-label">ToTime</label>
    				  <input type="text" class="form-control" name="toTs" id="toTs" value="<%= moment().format('YYYY-MM-DD') %>">
                  </div>
				  
				  <div class="form-group">
						<label for="country" class="control-label">Country</label>
						<select class="form-control" name="country" id="country">
						<% include country-options %>
						</select>
                  </div>
				  
				  <div class="form-group">
						<label for="pg" class="control-label">Page</label>
						<input type="text" class="form-control" style="width:220px" name="pg" id="pg" value="all">
                  </div>
				  
    			  <button type="submit" class="btn btn-primary">Add</button>
                </form>
            </div>
		</div>
		
		 <div class="row">
    	   <div class="col-md-12"   >
				<hr class="featurette-divider">
				<div class="alert alert-info" style="display:none" id="chartInfoDiv">Click on the chart legend to show, hide.</div>
    			<div  style="height:350px" id="chartDiv">
     			</div>
           </div>
      	 </div>
		 
       </div> 
	</div>
	
    </div> <!-- end container -->
    <script type="text/javascript" language="javascript" src="/static/flot-0.8.2/jquery.flot.min.js"></script>
	<script type="text/javascript" language="javascript" src="/static/flot-0.8.2/jquery.flot.time.min.js"></script>
	<script type="text/javascript" language="javascript" src="/static/flot-0.8.2/jquery.flot.hiddengraphs.js"></script>
	<script type="text/javascript" language="javascript" src="/static/flot-0.8.2/jquery.flot.tooltip.min.js"></script>
	<script type="text/javascript" language="javascript" src="/static/js/moment-2.5.1.min.js"></script>
	<script type="text/javascript" language="javascript" src="/static/js/page-response-analysis.js"></script>
	
  </body>
</html>