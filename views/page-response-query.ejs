<!DOCTYPE html>
<html lang="en">
  
  <head>
	<% include head %>
    <title>Page Response Query</title>
	<link href="static/jquery.dataTables/1.9.4/css/jquery.dataTables.css" rel="stylesheet">
  </head>
  
  <body>	
	<% include nav %>
	<div class="container-lg">
		<div class="row">
			<div class="col-md-12"   >
    			<h3>Page Response Time Query</h3>
				<hr class="featurette-divider">
    			<form id="form" class="form-inline" role="form">
                  <div class="form-group">
    				  <label for="fromTs" class="control-label">From Time</label>
                      <input type="text" class="form-control" name="fromTs" id="fromTs" value="<%= moment().format('YYYY-MM-DD') %>">
                  </div>
   
                  <div class="form-group">
    				  <label for="toTs" class="control-label">To Time</label>
    				  <input type="text" class="form-control" name="toTs" id="toTs" value="<%= moment().format('YYYY-MM-DD') %>">
                  </div>
				  <div class="form-group">
    				  <label for="tt" class="control-label">Response Time (ms) >= </label>
    				  <input type="text" class="form-control" name="tt" id="tt" value="0">
                  </div>
    			  <button type="submit" class="btn btn-primary">Submit</button>
                </form>
            </div>
		</div>
		
		<div id="tableChartDiv" style="display:none">
    		<div class="row">
				<div class="col-md-12"   >
					<hr class="featurette-divider">
					<p class="alert alert-info" id="tableTypeHeading"></p>
				</div>
              
        	   <div class="col-md-12"   >
            		<table class="table table-bordered display" id="dataTable">
            			<thead>
            				<tr>
            					<th title="Page">Page</th>
        						<th title="Country">Country</th>
            					<th title="Time">Time</th>
            					<th title="Page Count">CNT</th>
            					<th title="Total Page Response Time">TT</th>
        						<th title="Redirection Time">RD</th>
        						<th title="DNS Lookup Time">DNS</th>
        						<th title="TCP Connection Setup Time">CON</th>
								<th title="First Byte Receive Time From Server">FB</th>
        						<th title="Server Time">ST</th>
        						<th title="Download Time">DWN</th>
        						<th title="DOM Setup Time">DOM</th>
        						<th title="Load Time">LD</th>
        						<th>TableType</th>
            				</tr>
            			</thead>
            			<tbody>
            			</tbody>
            		</table>
    			</div>
          	 </div> <!-- end row -->
       </div> 
	   
    </div> <!-- end container -->
	
	<script type="text/javascript" charset="utf8" src="/static/jquery.dataTables/1.9.4/js/jquery.dataTables.min.js"></script>
    <script type="text/javascript" charset="utf8" src="/static/jquery.dataTables/1.9.4/js/dataTables.fnReloadAjax.js"></script>
	<script type="text/javascript" language="javascript" src="/static/js/moment-2.5.1.min.js"></script>
	<script type="text/javascript" language="javascript" src="/static/js/page-response-query.js"></script>
	
  </body>
</html>